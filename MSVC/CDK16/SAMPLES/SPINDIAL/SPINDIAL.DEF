LIBRARY      'SPINDIAL'
DESCRIPTION  'SPINDIAL OLE Custom Control module'
EXETYPE      WINDOWS

CODE         PRELOAD MOVEABLE DISCARDABLE
DATA         PRELOAD MOVEABLE SINGLE

HEAPSIZE     1024   ; initial heap size

EXPORTS
	WEP                 @1  RESIDENTNAME
	DLLCANUNLOADNOW     @2  RESIDENTNAME
	DLLGETCLASSOBJECT   @3  RESIDENTNAME
	DLLREGISTERSERVER   @4  RESIDENTNAME
	DLLUNREGISTERSERVER @5  RESIDENTNAME

SEGMENTS
    WEP_TEXT FIXED
